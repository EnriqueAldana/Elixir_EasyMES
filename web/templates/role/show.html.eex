<h2>Show role</h2>
<table class="table">
<td>
<%= link "Edit", to: role_path(@conn, :edit, @role) %>
</td>
<td>
<%= link "Back", to: role_path(@conn, :index) %>
</td>
</table>

<ul>

  <li>
    <strong>Role id:</strong>
    <%= @role.id %>
  </li>

  <li>
    <strong>Key role:</strong>
    <%= @role.key_role %>
  </li>

  <li>
    <strong>Description:</strong>
    <%= @role.description %>
  </li>

</ul>
